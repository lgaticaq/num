<%= form_for(@resource) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :dni %>
    <%= f.text_field :dni, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :number %>
    <%= f.text_field :number, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :account_type_id %>
    <%= collection_select :form_account_user, :account_type_id, AccountType.all, :id, :name, {}, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :bank_id %>
    <%= collection_select :form_account_user, :bank_id, Bank.all, :id, :name, {}, class: "form-control" %>
  </div>

  <div>
    <button class="btn btn-primary"><i class="fa fa-floppy-o"></i> Guardar</button>
  </div>
<% end %>
